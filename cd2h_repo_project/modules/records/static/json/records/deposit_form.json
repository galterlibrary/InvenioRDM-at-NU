  {
    "title": "Subjects",
    "type": "fieldset",
    "fieldhint": "optional",
    "items": [
      {
        "key": "mesh_terms",
        "fa_cls": "fa-tag",
        "title": "Medical",
        "type": "uiselectmultiple",
        "options": {
          "multiple": true,
          "asyncCallback": "autocompleteSuggest",
          "url": "/api/terms/mesh/_suggest",
          "urlParameters": {
            "q": "value",
            "limit": "7"
          },
          "map": {
            "resultSource": "terms",
            "valueProperty": "value",
            "nameProperty": "name"
          },
          "refreshDelay": 200
        },
        "placeholder": "Select Medical Subject Heading (MeSH) terms",
        "description": "Optional. Medical Subject Heading (MeSH) terms."
      },
      {
        "key": "fast_terms",
        "fa_cls": "fa-tag",
        "title": "Topical",
        "type": "uiselectmultiple",
        "options": {
          "multiple": true,
          "asyncCallback": "autocompleteSuggest",
          "url": "/api/terms/fast/_suggest",
          "urlParameters": {
            "q": "value",
            "limit": "7"
          },
          "map": {
            "resultSource": "terms",
            "valueProperty": "value",
            "nameProperty": "name"
          },
          "refreshDelay": 200
        },
        "placeholder": "Select Library of Congress topical terms",
        "description": "Optional. Library of Congress topical terms."
      }
    ],
    "required": false
